toc=savedreactivedata$final_output$quantification
toc[savedreactivedata$final_output$fitting_error>0.05]=NA
ab=rbind(toc[23:33,]-
           toc[1:11,],
         toc[34:44,]-
           toc[12:22,])
abc=p_values(ab,cbind(1:22,rep(c(1,2),c(11,11))))
ind=is.na(savedreactivedata$final_output$quantification[1,])
library(writexl)

sheets <- list("quantification" = as.data.frame(savedreactivedata$final_output$quantification[,!ind]),
               "fitting_error" = as.data.frame(savedreactivedata$final_output$quantification[,!ind]), #assume sheet1 and sheet2 are data frames
               "signal_area_ratio" = as.data.frame(savedreactivedata$final_output$signal_area_ratio[,!ind]),
               "PQN_factor"=as.data.frame(savedreactivedata$imported_data$norm_factor)) #assume sheet1 and sheet2 are data frame
write_xlsx(sheets,"C:/Bruker/TopSpin3.2/data/MalageladaC_P2832/data_analysis_r/results_10_02.xlsx")
